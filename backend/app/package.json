{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "prestart": "npm run build",
    "predev": "npm run build && npm run db:reset",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules ./src/server.ts",
    "start": "node ./dist/server.js",
    "db:reset": "npx prisma migrate reset --force --skip-generate",
    "compose:up:dev": "(cd ../.. && docker-compose -f docker-compose.yml up -d --build)",
    "compose:down:dev": "(cd ../.. && docker-compose -f docker-compose.yml down --remove-orphans)"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@tsconfig/node14": "^1.0.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.1",
    "nodemon": "^2.0.16",
    "prisma": "^4.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
